{
  "name": "campaignkeeper",
  "version": "1.0.0",
  "description": "",
  "main": "dist/Index.js",
  "scripts": {
    "test": "jest --runInBand",
    "copy-images": "copyfiles -f src/Common/Image/* dist/Common/Image",
    "build": "tsc && npm run copy-images",
    "clear": "rimraf -rf dist/",
    "prestart:dev": "npm run clear && npm run build",
    "prestart:dev:watch": "npm run clear",
    "start": "node dist/Index.js",
    "start:dev": "node dist/Index.js",
    "run:dev:watch": "npm run copy-images && npm run start",
    "start:dev:watch": "tsc-watch --outDir ./dist --onSuccess \"npm run run:dev:watch\"",
    "lint": "eslint \"./src/**/*.{ts,js}\"",
    "format": "npm run lint -- --fix",
    "migration:generate": "typeorm migration:generate -n",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Motii1/CampaignKeeper.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Motii1/CampaignKeeper/issues"
  },
  "homepage": "https://github.com/Motii1/CampaignKeeper#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "express-swagger-generator": "^1.1.17",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mssql": "^7.2.1",
    "multer": "^1.4.3",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.38",
    "typeorm-naming-strategies": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.2.0",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.3",
    "sqlite3": "^5.0.8",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.3"
  }
}
